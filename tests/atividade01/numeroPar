import { NumerosPares } from './numeroPar.js.js';

// Teste para a função getEvenNumbers
test('retorna apenas números pares', () => {
  expect( NumerosPares([1, 2, 3, 4, 5, 6])).toEqual([2, 4, 6]);
  expect( NumerosPares([11, 13, 15, 17])).toEqual([]);
  expect( NumerosPares([0, -2, -4, -6])).toEqual([0, -2, -4, -6]);
});
